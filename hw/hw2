mkdir u 
mv ../pa2/start/e/e/n/d/d/sw/w/d/e/d/n/n/u/u ../pa2/u 
mv ../pa2/u ../pa2/start/e/e/n/d/d/sw/w/d/e/d/n/n/u 
cd pa2/start/e/e/n/d/d/sw/w/d/e/d/n/n/u/u
